      "source": [
        "import tensorflow as tf\n",
        "from tensorflow.keras.layers import Input, Conv2D, ReLU, MaxPooling2D, Flatten, Dense, Dropout, BatchNormalization, Add\n",
        "from tensorflow.keras.initializers import GlorotUniform\n",
        "from sklearn.svm import SVC\n",
        "from sklearn.pipeline import make_pipeline\n",
        "from sklearn.preprocessing import StandardScaler\n",
        "\n",
        "init = GlorotUniform()\n",
        "\n",
        "# Define input layer\n",
        "inputss = Input(shape=(256, 256, 3))\n",
        "\n",
        "# Define a residual block function\n",
        "def enhance_block(x, filters, kernel_size, strides=(1, 1), activation='relu'):\n",
        "    y = Conv2D(filters, kernel_size, strides=strides, padding='same', kernel_initializer=init)(x)\n",
        "    y = BatchNormalization()(y)\n",
        "    y = ReLU()(y)\n",
        "    y = Conv2D(filters, kernel_size, strides=strides, padding='same', kernel_initializer=init)(y)\n",
        "    y = BatchNormalization()(y)\n",
        "    if strides != (1, 1) or x.shape[-1] != filters:\n",
        "        x = Conv2D(filters, (1, 1), strides=strides, padding='same', kernel_initializer=init)(x)\n",
        "    y = Add()([x, y])\n",
        "    y = ReLU()(y)\n",
        "    return y\n",
        "\n",
        "def residual_block(x, filters, kernel_size, strides=(1, 1), activation='relu'):\n",
        "    y =Conv2D(16, 5, kernel_initializer=init, activation='relu')(x)\n",
        "    y = Conv2D(filters, kernel_size, strides=strides, padding='same', kernel_initializer=init)(y)\n",
        "    y = BatchNormalization()(y)\n",
        "    y = MaxPooling2D(pool_size=(2, 2))(y)\n",
        "    y = ReLU()(y)\n",
        "    return y\n",
        "\n",
        "# First stage\n",
        "\n",
        "x = Conv2D(16, 5, kernel_initializer=init, activation='relu')(inputss)\n",
        "res=inputss\n",
        "x = MaxPooling2D(pool_size=(2, 2))(x)\n",
        "x = BatchNormalization()(x)\n",
        "x = enhance_block(x, filters=16, kernel_size=(3, 3))\n",
        "y=residual_block(res, filters=16, kernel_size=(3, 3))\n",
        "x = Add()([x, y])\n",
        "print(\"1.X.shape\",x.shape)\n",
        "inputs=x\n",
        "\n",
        "# Second stage\n",
        "x = Conv2D(16, 5, kernel_initializer=init, activation='relu')(inputs)\n",
        "res=inputs\n",
        "x = MaxPooling2D(pool_size=(2, 2))(x)\n",
        "x = BatchNormalization()(x)\n",
        "x = enhance_block(x, filters=16, kernel_size=(3, 3))\n",
        "y=residual_block(res, filters=16, kernel_size=(3, 3))\n",
        "x = Add()([x, y])\n",
        "print(\"2.X.shape\",x.shape)\n",
        "inputs=x\n",
        "\n",
        "# Third stage\n",
        "x = Conv2D(16, 5, kernel_initializer=init, activation='relu')(inputs)\n",
        "res=inputs\n",
        "x = MaxPooling2D(pool_size=(2, 2))(x)\n",
        "x = BatchNormalization()(x)\n",
        "x = enhance_block(x, filters=16, kernel_size=(3, 3))\n",
        "y=residual_block(res, filters=16, kernel_size=(3, 3))\n",
        "x = Add()([x, y])\n",
        "print(\"3.X.shape\",x.shape)\n",
        "inputs=x\n",
        "\n",
        "# Fourth stage\n",
        "x = Conv2D(16, 5, kernel_initializer=init, activation='relu')(inputs)\n",
        "res=inputs\n",
        "x = MaxPooling2D(pool_size=(2, 2))(x)\n",
        "x = BatchNormalization()(x)\n",
        "x = enhance_block(x, filters=16, kernel_size=(3, 3))\n",
        "y=residual_block(res, filters=16, kernel_size=(3, 3))\n",
        "x = Add()([x, y])\n",
        "inputs=x\n",
        "print(\"4.X.shape\",x.shape)\n",
        "\n",
        "# Fifth stage\n",
        "x = Conv2D(16, 5, kernel_initializer=init, activation='relu')(inputs)\n",
        "res=inputs\n",
        "x = MaxPooling2D(pool_size=(2, 2))(x)\n",
        "x = BatchNormalization()(x)\n",
        "x = enhance_block(x, filters=16, kernel_size=(3, 3))\n",
        "y=residual_block(res, filters=16, kernel_size=(3, 3))\n",
        "x = Add()([x, y])\n",
        "print(\"5.X.shape\",x.shape)\n",
        "\n",
        "# Flatten layer\n",
        "x = Flatten()(x)\n",
        "\n",
        "# Fully connected layers\n",
